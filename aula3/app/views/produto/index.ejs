<html>
    <body>
        
        <h1>Produtos</h1>

        <table>
            <tr>
                <th>Cod.Barras</th>
                <th>Produto</th>
                <th>Valor</th>
            </tr>

            <% for(var i=0; i<produtos.length; i++){ %>

                <tr>
                    <td><%= produtos[i].codBar %></td>
                    <td><%= produtos[i].nmProduto %></td>
                    <td><%= produtos[i].valor %></td>
                </tr>
            <% } %>
        </table>

    </body>
</html>